include ../Makefile

a	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DDIRECT -DIEQJ
	./a.out

a1	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DDIRECT
	./a.out

a2	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DDIRECT -DCART
	./a.out

a3	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DDIRECT -DSPHE
	./a.out

b	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DIEQJ
	./a.out

b1	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY
	./a.out

b2	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DCART
	./a.out

b3	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DSPHE
	./a.out

c	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DDIRECT -DIEQJ
	./a.out

c1	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DDIRECT
	./a.out

c2	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DDIRECT -DCART
	./a.out

c3	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DMANY -DDIRECT -DSPHE
	./a.out

d	: main.cxx
	make cleanall
	$(CXX) $? $(LFLAGS) -DBUILD
	./a.out

